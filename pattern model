import java.util.*;
import java.util.Scanner;
public class pattern{
    public static void main(String[] args){
        Scanner obj = new Scanner(System.in);
        int num = obj.nextInt();
      for(int row=1;row<=num;row++){
        for(int col=1;col<=num;col++){
            if(row==1||col == 1||row==num||col ==num)
            System.out.print("*");
            else
            System.out.print(" ");
        }
        System.out.println();
    }
}
}
